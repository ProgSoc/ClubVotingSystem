		"express": "^4.21.2",
		"gel": "^2.1.1",
		"nanoid": "^3.3.11",
		"ws": "^8.18.3",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@gel/generate": "^0.7.0",
		"@types/bun": "^1.3.1",
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.24",
		"@types/node": "^20.19.23",
		"@types/ws": "^8.18.1",
