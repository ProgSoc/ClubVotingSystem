{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-dev\"",
    "watch-compile": "swc src -w -s --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/server.js",
    "prisma": "dotenv -e ../../.env -- prisma",
    "build": "yarn prisma generate && tsc && swc src -d dist && cp -r src/prisma/migrations dist/prisma/migrations && cp -r src/prisma/schema.prisma dist/prisma/schema.prisma",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@trpc/server": "^9.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "nanoid": "^3.3.4",
    "prisma": "^4.4.0",
    "ws": "^8.9.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.3",
    "@types/ws": "^8.5.3",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.4.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}